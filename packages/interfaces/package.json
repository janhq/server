{
  "name": "@janhq/interfaces",
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://jan.ai/",
  "description": "Shared UI component library for Jan",
  "author": "Jan service@jan.ai",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/janhq/jan-server.git",
    "directory": "packages/interfaces"
  },
  "keywords": [
    "jan",
    "interfaces"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    "./lib": {
      "types": "./src/lib/utils.ts",
      "default": "./src/lib/utils.ts"
    },
    "./hooks": {
      "types": "./src/hooks/use-mobile.ts",
      "default": "./src/hooks/use-mobile.ts"
    },
    "./hooks/use-mobile": {
      "types": "./src/hooks/use-mobile.ts",
      "default": "./src/hooks/use-mobile.ts"
    },
    "./hooks/use-is-mobile-device": {
      "types": "./src/hooks/use-is-mobile-device.ts",
      "default": "./src/hooks/use-is-mobile-device.ts"
    },
    "./hooks/use-resolved-media-url": {
      "types": "./src/hooks/use-resolved-media-url.ts",
      "default": "./src/hooks/use-resolved-media-url.ts"
    },
    "./providers/*": {
      "types": "./src/providers/*.tsx",
      "default": "./src/providers/*.tsx"
    },
    "./providers/theme-provider": {
      "types": "./src/providers/theme-provider.tsx",
      "default": "./src/providers/theme-provider.tsx"
    },
    "./ai-elements/*": {
      "types": "./src/ai-elements/*.tsx",
      "default": "./src/ai-elements/*.tsx"
    },
    "./constants": {
      "types": "./src/lib/constants.ts",
      "default": "./src/lib/constants.ts"
    },
    "./svgs/*": {
      "types": "./src/svgs/*.tsx",
      "default": "./src/svgs/*.tsx"
    },
    "./ui/*": {
      "types": "./src/ui/*.tsx",
      "default": "./src/ui/*.tsx"
    },
    "./components/*": {
      "types": "./src/ui/*.tsx",
      "default": "./src/ui/*.tsx"
    },
    "./*": {
      "types": "./src/ui/*.tsx",
      "default": "./src/ui/*.tsx"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "ai": "5.0.108",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "nanoid": "^5.1.6",
    "shiki": "^3.19.0",
    "sonner": "^2.0.7",
    "streamdown": "npm:@janhq/streamdown@^1.1.6",
    "tailwind-merge": "^3.4.0",
    "use-stick-to-bottom": "^1.1.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}
